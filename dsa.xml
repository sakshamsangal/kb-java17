    <template name="sliding-widow" value="int start = 0;&#10;int sum = 0;&#10;int $currMin$;&#10;for (int i = 0; i &lt; $nums$.length; i++) {&#10;    &#10;}&#10;&#10;return $a$;" description="" toReformat="true" toShortenFQNames="true">
